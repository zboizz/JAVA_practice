<%@ page contentType="text/html; charset=EUC-KR"%>
<%
	pageContext.setAttribute("J", 31);		
	pageContext.setAttribute("S", 8);		
	pageContext.setAttribute("P", 22);		
%>
<b>
J=${J}, S=${S}, P=${P}
</b><p/>

<b>산술연산자</b>
<hr width="250" align="left">
&#36;{J+S} = ${J+S}<br/>
&#36;{J-S} = ${J-S}<br/>
&#36;{J*S} = ${J*S}<br/>
&#36;{J/S} = ${J/S}<br/>
&#36;{J%S} = ${J%S}<br/>
<br/>

<b>비교연산자</b>
<hr width="250" align="left">
&#36;{J < S} = ${J lt S}<br/> <!-- lt=less than -->
&#36;{J > S} = ${J gt S}<br/> <!--  gt=great than-->
&#36;{J <= S} = ${J le S}<br/> <!-- le=less equal -->
&#36;{J >= S} = ${J ge S}<br/> <!-- great equal -->

&#36;{(10*10)==100} = ${(10*10) eq 100} <br/>
&#36;{(10*10)!=100} = ${(10*10) ne 100} <br/>

<b>논리 및 조건 연산자</b>
<hr width="250" align="left">
&#36;{J > P && P < S} : ${J>P and P<S}<br/>
&#36;{J > P || P < S} : ${J>P and P<S}<br/>
&#36;{!(J == P)} : ${not(J==P}<br/>
&#36;{(J == P)?"같다":"다르다"} : ${J>P and P<S}<br/>




